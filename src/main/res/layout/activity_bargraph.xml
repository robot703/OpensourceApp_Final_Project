<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_dashboard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.example.shashank.login.bargraph_web"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF">

    <WebView
        android:id="@+id/webView2"
        android:layout_width="match_parent"
        android:layout_height="800dp"
        android:layout_marginTop="50dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavi3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:backgroundTint="#FFD147"
        app:itemIconTint="#000000"
        app:itemTextColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/main_menu_bottom_tap"
        android:layout_marginTop="810dp"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="80dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colortitle1"
            android:text="Bargraph View"
            android:textColor="#000000"
            android:textSize="30dp"
            android:textStyle="bold"
            android:gravity="center"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/button4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="start|center_vertical"
            android:src="@drawable/logout"
            android:background="@null"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="-15dp"/>

    </FrameLayout>
</RelativeLayout>
